entrada = ""
while entrada != "salir":
    entrada = input("Ingrese lo que quiera: \n")
    print(entrada)

# SOLUCION  DE https://aprendeconalf.es/
while True:
    frase = input("Introduce algo: ")
    if frase == "salir":
        break
    print(frase)
